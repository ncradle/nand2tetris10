{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "g++.exe build active file",
			"command": "C:\\msys64\\mingw64\\bin\\g++.exe",
			"args": [
				"-std=c++2a",
				"-Wall",
				"-Wextra",
				"-Winit-self",
				"-Wno-missing-field-initializers",
				"-g",
				"-o",
				"JackAnalyzer",
				"JackAnalyzer.cpp"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}
